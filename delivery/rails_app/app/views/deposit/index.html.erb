<div class="row">
  <div class="span4">
    <table id=deposits class="table table-bordered">
      <% @deposits.each do |t| %>
        <tr id=deposit_<%= t[:id] %>>
          <td> <%= link_to t[:description], edit_deposit_path(t[:id]) %></td><td class='amount'> <%=t[:amount] %> </td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
